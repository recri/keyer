<!DOCTYPE patchbay>
<patchbay name="key+out" version="0.5.0">
 <output-sockets>
  <socket client="kyo" type="jack-audio" name="kyo" exclusive="off">
   <plug>out_i</plug>
   <plug>out_q</plug>
  </socket>
  <socket client="system" type="jack-midi" name="system" exclusive="off">
   <plug>midi_capture_1</plug>
  </socket>
  <socket client="outo" type="jack-midi" name="detone" exclusive="off">
   <plug>midi_out</plug>
  </socket>
  <socket client="key" type="jack-midi" name="key" exclusive="off">
   <plug>midi_out</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket client="system" type="jack-audio" name="system" exclusive="off">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket client="key" type="jack-midi" name="key" exclusive="off">
   <plug>midi_in</plug>
  </socket>
  <socket client="outi" type="jack-midi" name="out" exclusive="off">
   <plug>midi_in</plug>
  </socket>
  <socket client="outo" type="jack-audio" name="detone" exclusive="off">
   <plug>in_i</plug>
  </socket>
  <socket client="kyo" type="jack-midi" name="kyo" exclusive="off">
   <plug>midi_in</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable type="jack-audio" output="kyo" input="system"/>
  <cable type="jack-audio" output="kyo" input="detone"/>
  <cable type="jack-midi" output="system" input="key"/>
  <cable type="jack-midi" output="detone" input="out"/>
  <cable type="jack-midi" output="key" input="kyo"/>
 </cables>
</patchbay>

