<!DOCTYPE patchbay>
<patchbay name="key" version="0.5.0">
 <output-sockets>
  <socket name="system" client="system" type="jack-midi" exclusive="off">
   <plug>midi_capture_1</plug>
  </socket>
  <socket name="key" client="key" type="jack-midi" exclusive="off">
   <plug>midi_out</plug>
  </socket>
  <socket name="kyo" client="kyo" type="jack-audio" exclusive="off">
   <plug>out_i</plug>
   <plug>out_q</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket name="key" client="key" type="jack-midi" exclusive="off">
   <plug>midi_in</plug>
  </socket>
  <socket name="kyo" client="kyo" type="jack-midi" exclusive="off">
   <plug>midi_in</plug>
  </socket>
  <socket name="system" client="system" type="jack-audio" exclusive="off">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable output="system" type="jack-midi" input="key"/>
  <cable output="key" type="jack-midi" input="kyo"/>
  <cable output="kyo" type="jack-audio" input="system"/>
 </cables>
</patchbay>

